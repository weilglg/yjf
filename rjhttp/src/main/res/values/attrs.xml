        <attr name="l_rightDividerWidth" />
        <attr name="l_rightDividerColor" />
        <attr name="l_rightDividerInsetTop" />
        <attr name="l_rightDividerInsetBottom" />
        <attr name="l_radius" />
        <attr name="l_borderColor" />
        <attr name="l_borderWidth" />
        <attr name="l_outerNormalColor" />
        <attr name="l_hideRadiusSide" />
        <attr name="l_showBorderOnlyBeforeL" />
        <attr name="l_shadowElevation" />
        <attr name="l_shadowAlpha" />
        <attr name="l_outlineInsetTop" />
        <attr name="l_outlineInsetLeft" />
        <attr name="l_outlineInsetRight" />
        <attr name="l_outlineInsetBottom" />
        <attr name="l_outlineExcludePadding" />
    </declare-styleable>

</resources>
